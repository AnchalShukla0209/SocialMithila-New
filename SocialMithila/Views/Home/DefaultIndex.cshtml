    <script>
        $(document).ready(function () {
            var $mainSearch = $("#mainSearch");
            var $navbarPlaceholder = $("#navbarSearchPlaceholder");

            $(window).scroll(function () {
                if ($(this).scrollTop() > 150) {
                    if ($navbarPlaceholder.is(":empty")) {
                        $mainSearch.find(".d-flex").clone(true, true).appendTo($navbarPlaceholder);
                    }
                } else {
                    $navbarPlaceholder.empty();
                }
            });

            $('.movie-slider').slick({
                slidesToShow: 4,
                slidesToScroll: 1,
                infinite: false,
                arrows: true,
                prevArrow: '<button type="button" class="slick-prev"><i class="ti-angle-left icon-nav"></i></button>',
                nextArrow: '<button type="button" class="slick-next"><i class="fa-solid fa-chevron-right  ti-angle-right icon-nav"></i></button>',
                responsive: [
                    { breakpoint: 1200, settings: { slidesToShow: 3 } },
                    { breakpoint: 768, settings: { slidesToShow: 2 } },
                    { breakpoint: 480, settings: { slidesToShow: 1 } }
                ]
            });
        });

        $(".icon-card").hover(function () {
            $(this).css("border-color", "#b5025a");
        }, function () {
            $(this).css("border-color", "#ddd");
        });

        $('#trendingCarousel').carousel({
            interval: 5000
        });


    </script>

